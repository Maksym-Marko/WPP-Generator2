{
	"name": "mxsfwn-npm-handler",
	"version": "1.3.0",
	"description": "NPM Handler",
	"author": "Maksym Marko",
	"license": "GPL-2.0-or-later",
	"scripts": {
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"build": "npm-run-all build:css build:js",
		"build:css": "postcss assets/css/style.css -o build/css/style.css",
		"watch:css": "onchange \"**/*.{php,html,js,jsx,ts,tsx}\" -- npm run build:css",
		"build:js": "wp-scripts build --config webpack.gutenberg.config.js",
		"watch:js": "wp-scripts start --config webpack.gutenberg.config.js",
		"start": "npm-run-all --parallel watch:css watch:js"
	},
	"scriptsForGutenbergCompelling": {},
	"scriptsForJavaScriptCompelling": {
		"watch": "webpack --watch --mode=development --config webpack.custom.config.js",
		"build": "webpack --mode=production --config webpack.custom.config.js"
	},
	"devDependencies": {
		"@babel/core": "^7.24.9",
		"@babel/preset-env": "^7.24.8",
		"@babel/preset-react": "^7.24.7",
		"@tailwindcss/postcss": "^4.1.14",
		"@wordpress/scripts": "^30.6.0",
		"babel-loader": "^9.1.3",
		"css-loader": "^7.1.2",
		"css-minimizer-webpack-plugin": "^7.0.0",
		"mini-css-extract-plugin": "^2.9.0",
		"npm-run-all": "^4.1.5",
		"onchange": "^7.1.0",
		"postcss": "^8.5.6",
		"postcss-cli": "^11.0.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"sass": "^1.80.6",
		"sass-loader": "^16.0.3",
		"style-loader": "^4.0.0",
		"tailwindcss": "^4.1.14",
		"terser-webpack-plugin": "^5.3.10",
		"webpack": "^5.93.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^5.0.4",
		"webpack-merge": "^6.0.1"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^2.3.0",
		"@wordpress/i18n": "^5.13.0",
		"axios": "^1.6.7",
		"clsx": "^2.1.1",
		"lodash": "^4.17.21",
		"react-redux": "^9.1.2",
		"react-router-dom": "^6.27.0",
		"redux": "^5.0.1",
		"sass": "^1.80.6",
		"uuid": "^11.1.0"
	}
}

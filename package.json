{
	"name": "mxsfwn-npm-handler",
	"version": "0.1.0",
	"description": "NPM Handler",
	"author": "Maksym Marko",
	"license": "GPL-2.0-or-later",
	"scripts": {
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"build": "wp-scripts build --webpack-src-dir=src/gutenberg/ --output-path=built/gutenberg/",
		"start": "wp-scripts start --webpack-src-dir=src/gutenberg/ --output-path=built/gutenberg/",
		"w-build": "webpack --mode production",
		"w-start": "webpack serve --mode production"
	},
	"devDependencies": {
		"@wordpress/scripts": "^26.17.0"
	},
	"dependencies": {
		"axios": "^1.6.7"
	}

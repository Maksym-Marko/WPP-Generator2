{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mxsfwn/content-slider",
  "version": "0.1.0",
  "title": "Content Slider",
  "category": "widgets",
  "icon": "format-gallery",
  "description": "Add a Content Slider with autoplay functionality",
  "supports": {
    "html": false,
    "align": [
      "left",
      "right",
      "full"
    ],
    "anchor": true,
    "color": {
      "gradients": true,
      "link": true,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    },
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "attributes": {
    "autoplay": {
      "type": "string",
      "source": "attribute",
      "selector": ".wp-block-mxsfwn-content-slider",
      "attribute": "data-autoplay-speed",
      "default": "0"
    },
    "showNavigation": {
      "type": "boolean",
      "default": true
    },
    "showDots": {
      "type": "boolean",
      "default": true
    },
    "loop": {
      "type": "boolean",
      "default": true
    }
  },
  "textdomain": "wpp-generator-next-source",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./editor.scss",
  "style": "file:./style.scss"
}